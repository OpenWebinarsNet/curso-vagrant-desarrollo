{{ form_start(form) }}

<div class="row">
    <div class="col-sm-6">
        {{ form_row(form.name) }}
    </div>
    <div class="col-sm-6">
        {{ form_row(form.slug) }}
    </div>
    <div class="col-sm-6">
        {{ form_row(form.category) }}
    </div>
    <div class="col-sm-6">
        {{ form_row(form.user) }}
    </div>
</div>
{{ form_widget(form) }}

{{ form_end(form) }}